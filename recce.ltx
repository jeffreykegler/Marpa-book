}
\begin{equation}
\label{eq:th-prefix-uniqueness-170}
\begin{gathered}
\exists\; \Rvarseq{pref1}{V} \in \Vmyfn{Upaths}{G} :
    \exists\; \Rvarseq{pref2}{V} \in \Vmyfn{Upaths}{G} :
\\ \Velement{pref1}{\Vlastix{pref1}} = \Velement{pref2}{\Vlastix{pref2}}
