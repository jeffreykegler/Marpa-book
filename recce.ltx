\begin{equation}
\label{eq:th-lim-non-duplication-36}
\begin{gathered}
    \mylim{\ell2} = \tuple{
         \begin{gathered}
         \DR{\Velement{max}{\Vlastix{max}}}, \Postdot{\var{src}},
