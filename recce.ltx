\begin{equation}
\label{eq:th-lim-non-duplication-34}
\begin{gathered}
    \mylim{\ell1} = \tuple{
         \begin{gathered}
         \DR{\Velement{max}{\Vlastix{max}}}, \Postdot{\var{src}},
         \\ \Current{\var{src}}, \Origin{\Velement{max}{\Vlastix{max}}}
         \end{gathered}
