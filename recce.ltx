\begin{definition}[Slice]
\label{def:slice}
we say that \V{slice} is a
\dfn[Slice (of a sequence)]{slice}
of size \V{len}
in \V{seq}
from \V{offset}
iff
\begin{gather}
\label{eq:def-slice-030}
        \V{offset}+\V{len} \le \Dom{\V{seq}} \text{ and}
\\ \label{eq:def-slice-040}
    \V{slice} =
    \set{ \begin{gathered}
        \V{term} \in \naturals\times\V{V} :
        \\ \Vtupelem{term}{0} \in \V{len}
        \\ \land\; \Vtupelem{term}{1} = \VVelement{seq}{\V{offset}+\Vtupelem{term}{0}}
    \end{gathered} }.
\end{gather}
We say that \V{offset} is the \dfn[Offset (of a slice)]{offset}.
We say that \V{len} is the \dfn[Length (of a slice)]{length}.
We also say that \V{len} is the \dfn[Size (of a slice)]{size}.
We say that \V{seq} is the \dfn{container sequence}%
\index{recce-defienda}{sequence@Sequence!container},
of \V{slice}
We say the \V{slice} is \dfn[from@From (of a slice)!from an index of container sequence]{from} the index
\V{offset}.
If \V{slice} is finite,
we say the \V{slice} is \dfn[from@To (of a slice)!to an index of container sequence]{to} the index
$\V{offset}+\Vdecr{len}$.
\dfEnd
\end{definition}

\renewcommand{\labBase}{lem-slice-helper}
\begin{lemma}[Slice helper]
\label{lem:slice-helper}
\begin{gather}
\Elab{010} \Vnat{ix} \in \Vnat{len}
\\ \Elab{030} \implies \V{offset}+\V{ix} \in \Dom{\V{seq}}.
    \quad \lemEnd
\end{gather}
\end{lemma}

\begin{proof}
The proof is direct,
assuming the hypothesis \E{010} to show the consequent \E{030}
\eqpar{120}{$\V{ix} < \V{len}$ \cuz{} Hypothesis of the theorem \E{010}, AF proof.}
We recall the context contains \V{slice} and \V{seq}, and that \V{ix} and \V{len}
are defined in terms of them, so that
\eqpar{125}{$\Vnat{offset}+\V{len} \le \Dom{\V{seq}}$
    \bcuz{} \Rmref[Slice context]{cxt-sec-slice}, \Dfref[Slice]{slice}.}
\eqpar{130}{ $\V{offset}+\V{ix} < \Dom{\V{seq}}$ \cuz{} \E{120}, \E{125}.}
\eqpar{140}{ $\V{offset}+\V{ix} \in \Dom{\V{seq}}$ \cuz{} \E{130}.}
\E{140} is the consequent, which is what we sought to show for
the current proof.
\myqed
\end{proof}

\renewcommand{\labBase}{th-slice-well-definedness}
\begin{theorem}[Slice well-definedness]
\label{th:slice-well-definedness}
\Dfref[Slice]{slice} is well-defined.
\thEnd
\end{theorem}

\begin{proof}
The only part of
\Dfref{slice} that is problematic is the indexing
$\VVelement{seq}{\V{offset}+\Vtupelem{term}{0}}$
in \Eref{def-slice-040}.
We need to show that the index
$\V{offset}+\Vtupelem{term}{0}$
is in the domain \V{seq}, which we do directly.

From the definition of slice we know that
the indexing in \Eref{def-slice-040} is guarded
by the left hand side of two conjunctions:
\eqpar{100}{$\V{offset}+\V{len} \le \Dom{\V{seq}}$
         \cuz{} \Eref{def-slice-030} in \Dfref{slice}.}
\eqpar{110}{$\Vtupelem{term}{0} \in \V{len}$
         \cuz{} \Eref{def-slice-040} in \Dfref{slice}.}
If we let set \V{ix}
in the hypothesis
of the Slice Helper Lemma \Lmref{slice-helper}
to \Vtupelem{term}{0},
we can conclude
\eqpar{130}{ $\V{offset}+\Vtupelem{term}{0} \in \Dom{\V{seq}}$
         \cuz{} \Lmref{slice-helper}, \E{100}, \E{110}.}
\E{130} is what we needed for the proof.
\myqed
\end{proof}

\begin{observation}[Vacuous slice]
From the definition of slice \Dfref{slice},
it is vacuously true that if \Vsize{len} = 0, then
\V{slice} is a slice of \V{seq}.
\obEnd
\end{observation}

\begin{observation}[Slice offset]
\label{obs:offset-of-a-slice}
Where \V{cntn} is a container sequence,
\V{offset} is the offset of a slice, \V{slice},
iff $\V{offset} \le \Vsize{cntn}$, or, alternatively,
$\V{offset} \in \V{cntn}+1$.
In plainer terms, the offset of a slice may be anywhere in its
container, or at the end.

We note that that, if \V{offset}=\Vsize{cntn},
that \VVelement{slice}{offset} is not well-defined.
In plainer terms, the container does not have a value at every possible slice offset.
Where the slice does not have a value at its offset, the slice must be empty.
\obEnd
\end{observation}

The following two theorems restate the definition of slice \Dfref{slice}
in helpful ways.

\renewcommand{\labBase}{th-slice-1}
\begin{theorem}[Slice 1]
\label{th:slice-1}
Where $\V{ix}\in\naturals$ and
$\V{v}\in\V{V}$,
\begin{gather}
\Elab{040}
    \tuple{\V{ix}, \V{v}} \in \V{slice} \iff
        \V{ix} \in \V{len}
        \land\; \V{v} = \Velement{seq}{\V{offset}+\V{ix}}.
            \quad \dfEnd
\end{gather}
\end{theorem}

\begin{proof}
The proof is direct, starting with the definition of slice \Dfref{slice},
to show \E{040}.
\eqpar{100}{$\V{term} = \tuple{\V{ix}, \V{v}}$
    \cuz{} CV $\V{term}\in\naturals\times\V{V}$.}
\begin{equation}
\begin{gathered}
\Elab{110} \V{term} \in \V{slice}
\\ \iff \Vtupelem{term}{0} \in \V{len}
        \land\; \Vtupelem{term}{1} = \VVelement{seq}{\V{offset}+\Vtupelem{term}{0}}
\\ \myparbox{\cuz{} \Dfref[Slice]{slice}.}
\end{gathered}
\end{equation}
Substituting from \E{100} in \E{110} yields \E{040},
which is what we sought to prove.
\myqed
\end{proof}

\renewcommand{\labBase}{th-slice-2}
\begin{theorem}[Slice 2]
\label{th:slice-2}
