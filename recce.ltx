\FloatBarrier
\begin{algorithm}[H]
\caption{\var{Earley-Reducer}}
\label{alg:earley-reducer-op}
\begin{algorithmic}[1]
\Procedure{Earley-Reducer}{\Ves{j},\Veimset{matches},\Veim{cuz}}
\For{every $\Veim{match} \in \Veimset{matches}$}
      \label{line:earley-reducer-op-2}
\State $\Veim{trial} = \tuple{\Next{\DR{\var{match}}},\Origin{\var{match}}}$
      \label{line:earley-reducer-op-3}
\State $\Vlp{lp} = \tuple{\Veim{match},\Veim{cuz}}$
      \label{line:earley-reducer-op-4}
\If{$\Seen{\Veim{trial}}$}
      \label{line:earley-reducer-op-5}
    \State Add \var{lp} to \LinkPairs{\var{trial}}
      \label{line:earley-reducer-op-6}
\Else
      \label{line:earley-reducer-op-7}
    \State $\Seen{\Veim{trial}} = \var{true}$
      \label{line:earley-reducer-op-8}
    \State $\LinkPairs{\var{trial}} = \set{\var{lp}}$
      \label{line:earley-reducer-op-9}
    \State Add \var{trial} to \Ves{j}
      \label{line:earley-reducer-op-10}
\EndIf
\EndFor
\EndProcedure
\end{algorithmic}
\end{algorithm}
\FloatBarrier

\begin{observation}[\op{Earley-Reducer} passes]
\label{obs:earley-reducer-passes}
The loop of lines
\enRefs{line:earley-reducer-op-2}{line:earley-reducer-op-10}
is the only one in
\op{Earley-Reducer}.
Every pass of loop
\enRefs{line:earley-reducer-op-2}{line:earley-reducer-op-10}
is
\begin{itemize}
\item exactly one EIM try pass; and
\item a new EIM pass or a dup EIM pass.  \dispEnd{}
\end{itemize}
\end{observation}

For the purposes of analyzing \op{Earley-Reducer},
``loop pass'', or just ``pass'',
refers to a pass of
loop \enRefs{line:earley-reducer-op-2}{line:earley-reducer-op-10}.

